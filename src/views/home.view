  $(document).ready(function () {
    $(window).scroll(function () {
      var scroll = $(window).scrollTop();
      var navbar = $(".navbar");
      if (scroll >= 100) {
        navbar.addClass("bg");
      } else {
        navbar.removeClass("bg");
      }
    });
    const menuIcon = $("#menu-icon");
    menuIcon.on("click", function () {
      $("body").toggleClass("mobileNav");
    });
  });
  AOS.init();
</script>
