body{
    background-color: rgb(241, 241, 241);
    font-family: 'poppins', sans-serif;
}
.navbar{
    height: 60px;
    letter-spacing: 3px;
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.navbar.bg{
    background-color: rgb(241, 241, 241);
}
.navbar li{
    list-style: none;
}
ul{
    margin: 0;
    padding: 0;
}
.icon span{
    color: magenta;
}
.nav a:focus{
    border: 1px solid magenta;
    color: white;
    background-color: rgb(82, 81, 81);
}
.nav a:hover{
    border: 1px solid magenta;
}
.main{
    min-height: 100vh;
}
.mainTitle{
    font-family: 'Dancing Script', cursive;
}
.shadow{
    width: 2000px;
    height: 1000px;
    bottom: -50px;
    background-color: #d7c9da;
}

.mainbtn{
    transition: all 0.5s ease-in-out;
    letter-spacing: 2px;
    display: inline-block;
}
.mainbtn::before {
    content: "";
    position: absolute;
    top: 0;
    left: -100%;
    width: 100%;
    height: 100%;
    background-color: #d7c9da;
    z-index: 1;
    transition: left 0.7s ease;
    border-radius: inherit;
  }
  
.mainbtn:hover::before {
    left: 0;
}
.mainDescription{
    font-family: 'Roboto', sans-serif;
    line-height: 25px;
}
.img{
    margin: 0 auto;
}
.sectionTitle{
    font-family: 'Dancing Script', cursive;
    font-size: 20px;
}
.point{
    width: 20px;
    height: 20px;
    background-color: #d7c9da;
    position: absolute;
    right: 0;
    translate: 50%;
    top: 50%;
}
.round{
    width: 30px;
    height: 30px;
    position: absolute;
    right: 0;
    translate: 50% -19%;
    top: 50%;
    border: 2px solid #d7c9da;
}
.title{
    font-family: 'Indie Flower', cursive;
}
.shadow2{
    height: 100px;
    border-radius: 50%;
    left: 50%;
    translate: -50%;
}
.text{
    letter-spacing: 1.2px;
}
.circle{
    width: 30px; height: 30px;
    left: 50%;
    translate: -50%;
    top: -10px;
}
.barbie{
    background-repeat: no-repeat !important;
    background-position: top right !important;
}
.fourth{
  min-height: 80vh;
}
.socialicon:hover{
  color: #d7c9da;
}
.boxContain:nth-child(1) .box {
    transform: rotate(3deg);
}
.boxContain:nth-child(2) .box {
    transform: rotate(0deg);
}
.boxContain:nth-child(3) .box {
    transform: rotate(-3deg);
}
.girlImage{
  translate: 50% -50%;
}
.girlcontent h3{
  letter-spacing: 1.5px;
}
.box{
  margin: 0 auto;
}
.sixth{
  min-height: 30vh;
  background-color: #d7c9da;
}
.circleBox{
  top: -60px;
  background-color: #d7c9da;
}
.lamp{
  background-repeat: no-repeat !important;
  background-position: center !important;
}
.seventhImg{
  background-repeat: no-repeat !important;
  background-position: center right !important;
}
.social-icons{
  width: 40px; height: 40px;
  background-repeat: no-repeat !important;
}
.sixth{
  padding-right: 20%;
}
.inputText{
  height: 35px;
}
.footerContent .title{
  border-bottom: 1px solid magenta;
  height: fit-content;
  width: fit-content;
}
.square{
  width: 150px;
  background-color: #d7c9da;
}
@media all and (max-width:1260px){
  .lamp{
    bottom: -50px !important;
  }
}
@media all and (max-width:1200px){
  .sixth{
    padding-right: 25%;
  }
}
@media all and (max-width:1024px){
  .sixth{
    padding-right: 33.3334%;
  }
}
@media all and (max-width:768px){
    .nav{
        display: none;
    }
    .navbar, .nav{
        background-color: rgb(241, 241, 241);
    }
    body.mobileNav .nav{
        top: 60px;
        display: block;
        left: 0;
        padding: 20px;
    }
    body.mobileNav .nav a{
        margin: 10px;
    }
    .mainImg{
        margin: 0 auto;
    }
    .secondLeft{
        float: none;
    }
    .secondLeft{
        border-left: 1px solid #dee2e6;
    }
    .footerContent:first-of-type{
        padding-right: 45%;
    }
    .sixth{
      padding-right: 0;
    }
    .seventhImg{
      background-position: center !important;
}
}
@media all and (max-width:480px){
  .footerContent:first-of-type{
    padding-top: 40%;
    padding-right: 0;
  }
  .content::before {
    content: "";
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 130%;
    background-image: url(../images/barbie.png);
    opacity: 0.2;
    background-repeat: no-repeat;
    background-position: top center;
  }
  .mainbtn{
    background-color: #d7c9da;
  }
  .mainbtn::before {
    background-color: black;
  }
  .square{
    width: 120px;
    background-color: #d7c9da;
}
}
